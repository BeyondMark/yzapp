.coupon-list-wrapper {
  background-color: #f2f3f5;
}
.coupon-list {
  padding: 0;
}
.coupon-list-empty {
  margin-top: 20px;
  text-align: center;
  font-size: 12px;
  color: #969799;
}
.coupon-list-title {
  display: flex;
  align-items: center;
  padding: 16px 0 12px;
  font-size: 14px;
  font-weight: 500;
}
.coupon-list-title .coupon-count-wrapper {
  display: flex;
}
.coupon-list-title .coupon-count {
  margin: 0 4px;
  color: #999;
}
