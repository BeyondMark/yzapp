.order-pay-prompt-popup-evaluation {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.evaluation {
  font-size: 12px;
  height: 24px;
  line-height: 24px;
  margin-bottom: 12px;
  margin-right: 12px;
}
.evaluation,
.evaluation-theme {
  border-radius: var(--theme-radius-tag, 16px);
}
.evaluation-theme {
  padding: 0 8px;
  color: #323233;
}
