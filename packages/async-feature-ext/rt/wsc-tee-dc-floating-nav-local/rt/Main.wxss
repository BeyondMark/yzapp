.tee-image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.theme-floating-nav {
  position: fixed;
  bottom: calc(80px + var(--buy-button-height, 0px));
  right: 10px;
  width: 48px;
  height: 48px;
  transition: all 0.24s;
  z-index: 99;
}
.theme-floating-nav.is-iphone-x {
  bottom: calc(114px + var(--buy-button-height, 0px));
}
.theme-floating-nav.hidden {
  transform: translate3d(100px, 0, 0);
}
.theme-floating-nav .is-tab {
  bottom: 30px;
}
.theme-floating-nav .line-contact-button {
  height: 48px;
}
.theme-floating-nav .item {
  position: absolute;
  top: 0;
  left: 0;
  width: 48px;
  height: 48px;
  border-radius: 50%;
  background-repeat: no-repeat;
  background-position: 50%;
  background-size: cover;
  transition: all 0.24s;
  overflow: visible;
  background-color: transparent;
  z-index: 2;
}
.theme-floating-nav .independent {
  opacity: 1;
  visibility: visible;
}
.theme-floating-nav .independent.line-only-independent {
  margin-top: 58px;
}
.theme-floating-nav .share-item {
  background-color: transparent;
  border: none;
  position: relative;
}
.theme-floating-nav .item:after,
.theme-floating-nav .share-item:after {
  display: none;
}
.theme-floating-nav .main {
  z-index: 4;
}
.theme-floating-nav .main-2 {
  opacity: 0;
  visibility: hidden;
  transform: rotate(-90deg);
}
.theme-floating-nav .skeleton {
  display: none;
}
.theme-floating-nav .line--only-independent .independent {
  margin-top: 58px;
}
.theme-floating-nav .has-badge:before {
  content: "";
  box-sizing: border-box;
  position: absolute;
  top: 10px;
  right: 10px;
  width: 8px;
  height: 8px;
  border: 1px solid #fff;
  border-radius: 50%;
  background-color: #f44;
}
.theme-floating-nav .tag {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 40px;
  border-radius: 8px;
  background-color: #ff4240;
  color: #fff;
  font-size: 10px;
  line-height: 16px;
  text-align: center;
}
.theme-floating-nav.active .main-1 {
  opacity: 0;
  visibility: hidden;
  transform: rotate(90deg);
}
.theme-floating-nav.active .main-2 {
  opacity: 1;
  visibility: visible;
  transform: none;
}
.theme-floating-nav .panel {
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  position: fixed;
  bottom: calc(100px + var(--buy-button-height, 0px));
  left: 50%;
  margin-left: -104px;
  width: 208px;
  padding: 10px 4px 0;
  border-radius: 24px;
  background-color: rgba(0, 0, 0, 0.7);
  opacity: 0;
  visibility: hidden;
  transform: translate3d(36vw, 158rpx, 0) scale3d(0, 0, 1);
  z-index: 3;
  transition: all 0.2s;
}
.theme-floating-nav .floating-main-bg {
  position: absolute;
  z-index: 3;
}
.theme-floating-nav .panel--three-rows {
  width: 228px;
  margin-left: -114px;
  padding: 8px 4px 3px;
}
.theme-floating-nav .is-iphone-x .panel {
  bottom: 134px;
}
.theme-floating-nav .is-tab .panel {
  bottom: 50px;
}
.theme-floating-nav .message-contact-button-item {
  flex: 1 1 30%;
  min-width: 30%;
}
.theme-floating-nav .panel-item {
  flex: 1 1 30%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  min-width: 30%;
  margin-bottom: 10px;
  height: 64px;
  background: transparent;
  font-size: 12px;
  line-height: 16px;
  color: #fff;
}
.theme-floating-nav .panel-item-message-button {
  margin-bottom: 0;
}
.theme-floating-nav .panel-item:after {
  display: none;
}
.theme-floating-nav .panel-item .item {
  position: relative;
  width: 36px;
  height: 36px;
  margin-bottom: 10px;
  box-shadow: none;
}
.theme-floating-nav .panel-item .has-badge:before {
  top: 6px;
  right: 6px;
}
.theme-floating-nav .outer-space {
  position: fixed;
  top: 0;
  left: 0;
  width: 0;
  height: 0;
}
.theme-floating-nav.active .panel {
  opacity: 1;
  visibility: visible;
  transform: none;
}
.theme-floating-nav.active .outer-space {
  width: 100%;
  height: 100%;
  z-index: 2;
}
.theme-floating-nav .share-share-corner {
  width: 20px;
  height: 16px;
  background: #ee0a24;
  border-radius: 8px;
  position: absolute;
  top: -1px;
  right: 0;
  color: #fff;
  font-size: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.message-floating-bg {
  position: absolute;
}
