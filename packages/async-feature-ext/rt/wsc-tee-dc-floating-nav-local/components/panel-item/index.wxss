.panel-item-warp {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  min-width: 30%;
  background: transparent;
  font-size: 12px;
  line-height: 16px;
  color: #fff;
}
.panel-item-warp .item {
  position: relative;
  top: 0;
  left: 0;
  width: 39px;
  height: 39px;
  border-radius: 50%;
  background-color: #fff !important;
  background-repeat: no-repeat;
  background-position: 50%;
  background-size: cover;
  box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.16);
  transition: all 0.24s;
  overflow: visible;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 10px;
}
.panel-item-warp .text {
  width: 100%;
  text-align: center;
  display: inline-block;
}
.panel-item-warp .item:after {
  display: none;
}
.panel-item-warp .has-badge:before {
  content: "";
  box-sizing: border-box;
  position: absolute;
  top: 10px;
  right: 10px;
  width: 8px;
  height: 8px;
  border: 1px solid #fff;
  border-radius: 50%;
  background-color: #f44;
}
