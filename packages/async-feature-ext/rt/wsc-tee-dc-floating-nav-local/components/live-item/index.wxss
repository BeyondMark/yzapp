.tee-image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
@keyframes wave {
  0% {
    height: 8px;
  }
  50% {
    height: 18px;
  }
  to {
    height: 8px;
  }
}
@keyframes wave_low {
  0% {
    height: 8px;
  }
  50% {
    height: 16px;
  }
  to {
    height: 8px;
  }
}
@keyframes wave_new {
  0% {
    height: 8.01rpx;
  }
  50% {
    height: 18.023rpx;
  }
  to {
    height: 8.01rpx;
  }
}
@keyframes wave_low_new {
  0% {
    height: 8.01rpx;
  }
  50% {
    height: 16.02rpx;
  }
  to {
    height: 8.01rpx;
  }
}
@keyframes circle_outside {
  0% {
    opacity: 1;
    transform: scale(1);
  }
  to {
    opacity: 0;
    transform: scale(1.1);
  }
}
@keyframes circle_inside {
  0% {
    transform: scale(1);
  }
  50% {
    transform: scale(0.9);
  }
  to {
    transform: scale(1);
  }
}
.channel-live-floating-new {
  position: fixed;
  top: 262.327rpx;
  right: 10.012rpx;
  z-index: 99;
}
.channel-live-floating-new .top {
  position: relative;
  width: 104.13rpx;
  height: 104.13rpx;
  padding: 10.413rpx;
  padding-bottom: 0;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
}
.channel-live-floating-new .circle-outside {
  animation: circle_outside 0.8s linear infinite;
}
.channel-live-floating-new .circle-middle,
.channel-live-floating-new .circle-outside {
  position: absolute;
  border-radius: 50%;
  border: 3.004rpx solid #ff2281;
  width: 104.13rpx;
  height: 104.13rpx;
  box-sizing: border-box;
  top: 10.413rpx;
  right: 10.413rpx;
}
.channel-live-floating-new .circle-inside {
  border-radius: 50%;
  border: 2.002rpx solid #ff2281;
  width: 88.11rpx;
  height: 88.11rpx;
  top: 8.01rpx;
  right: 8.01rpx;
  overflow: hidden;
  animation: circle_inside 1.6s linear infinite;
}
.channel-live-floating-new .circle-inside image {
  width: 88.11rpx;
  height: 88.11rpx;
}
.channel-live-floating-new .bottom {
  position: absolute;
  width: 96.12rpx;
  height: 30.037rpx;
  background-image: linear-gradient(146deg, #ff2282, #e23d49);
  border-radius: 15.019rpx;
  top: 86.508rpx;
  right: 14.418rpx;
  overflow: hidden;
}
.channel-live-floating-new .bottom image {
  width: 96.12rpx;
  height: 30.037rpx;
}
.channel-live-floating-new .live-icon-wrap {
  position: absolute;
  top: 0;
  left: 0;
  width: 16.02rpx;
  height: 18.023rpx;
}
.channel-live-floating-new .default-live-icon-wave {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 8.01rpx;
  left: 8.01rpx;
  display: flex;
  box-sizing: border-box;
  justify-content: center;
}
.channel-live-floating-new .default-live-icon-wave .wave-wrap {
  height: 18.023rpx;
  display: flex;
  align-items: flex-end;
  justify-content: center;
}
.channel-live-floating-new .default-live-icon-wave .wave-item {
  width: 4.005rpx;
  height: 8.01rpx;
  border-radius: 2.002rpx;
  background: #fff;
}
.channel-live-floating-new .default-live-icon-wave .wave-item + .wave-item {
  margin-left: 3.004rpx;
}
.channel-live-floating-new .default-live-icon-wave .wave-item.wave-1 {
  animation: wave_low_new 1s ease-in-out infinite;
}
.channel-live-floating-new .default-live-icon-wave .wave-item.wave-2 {
  animation: wave_new 1s ease-in-out 0.4s infinite;
}
.channel-live-floating-new .default-live-icon-wave .wave-item.wave-3 {
  animation: wave_low_new 1s ease-in-out 0.8s infinite;
}
.channel-live-floating {
  position: fixed;
  top: 136px;
  right: 16px;
  z-index: 99;
  width: 48px;
  height: 48px;
}
.custom-icon {
  width: 50px;
  height: 50px;
}
.custom-icon img {
  width: 100%;
  height: 100%;
}
.default-icon {
  position: absolute;
}
.default-icon,
.default-icon image {
  width: 48px;
  height: 52px;
}
.default-icon .default-live-icon-wave {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  display: flex;
  padding: 10px;
  box-sizing: border-box;
  justify-content: center;
}
.default-icon .default-live-icon-wave .wave-wrap {
  height: 18px;
  display: flex;
  align-items: flex-end;
  justify-content: center;
}
.default-icon .default-live-icon-wave .wave-item {
  width: 3px;
  height: 8px;
  border-radius: 1.5px;
  background: #fff;
}
.default-icon .default-live-icon-wave .wave-item + .wave-item {
  margin-left: 3px;
}
.default-icon .default-live-icon-wave .wave-item.wave-1 {
  animation: wave_low 1s ease-in-out infinite;
}
.default-icon .default-live-icon-wave .wave-item.wave-2 {
  animation: wave 1s ease-in-out 0.4s infinite;
}
.default-icon .default-live-icon-wave .wave-item.wave-3 {
  animation: wave_low 1s ease-in-out 0.8s infinite;
}
