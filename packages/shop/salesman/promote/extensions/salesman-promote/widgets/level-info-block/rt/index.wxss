.level-info-wrap .level-van-cell {
  margin-top: 8px;
  background: #f2f3f5;
  border-radius: 0 0 4px 4px;
}
.level-info-wrap .level-info__info-wrap {
  font-size: 12px;
  color: #646566;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.level-info-wrap .level-info__desc {
  display: inline;
}
.level-info-wrap .level-info__commission,
.level-info-wrap .level-info__commission-rate {
  display: inline;
  font-size: 16px;
  font-weight: 600;
  margin-left: 4px;
}
.level-info-wrap .level-info__commission {
  margin-left: 0;
}
