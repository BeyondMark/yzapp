.tee-image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
@font-face {
  font-family: yzicon;
  src: url(https://b.yzcdn.cn/brand-template/yzicon_6e5df2f173c387d65c4569cff617197b.svg#yzicon)
      format("svg"),
    url(https://file.yzcdn.cn/upload_files/yz-file/2021/05/13/FvzqMWn6DsIFK5QTDS9IATbYnOzw.ttf)
      format("truetype"),
    url(https://file.yzcdn.cn/upload_files/yz-file/2021/05/13/Frz_XXs8YK4wA4pci_8ZvXf_PsfD.eot#iefix)
      format("embedded-opentype"),
    url(https://file.yzcdn.cn/upload_files/yz-file/2021/05/13/FrJzBh7NW8JPXe9R0vhw3_w3Qw92.woff)
      format("woff"),
    url(https://file.yzcdn.cn/upload_files/yz-file/2021/05/13/FpSWda8jUeQT5bRYYgzcVXCy1bsj.woff2)
      format("woff2");
}
[class*="nav-icon-"],
[class^="nav-icon-"] {
  font-family: yzicon !important;
  display: block;
  speak: none;
  font-style: normal;
  font-weight: 400;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.nav-icon-back:before {
  content: "";
}
.nav-icon-home:before {
  content: "";
}
.nav-icon-menu:before {
  content: "";
}
.nav-icon-search:before {
  content: "";
}
@font-face {
  font-family: navigation-iconfont;
  src: url(https://b.yzcdn.cn/project/2020/top-nav-v2/yzicon_6e5df2f173c387d65c4569cff617197b.ttf)
      format("truetype"),
    url(https://b.yzcdn.cn/project/2020/top-nav-v2/yzicon_6e5df2f173c387d65c4569cff617197b.woff)
      format("woff"),
    url(https://b.yzcdn.cn/project/2020/top-nav-v2/yzicon_6e5df2f173c387d65c4569cff617197b.woff2)
      format("woff2");
}
.nb {
  width: 100%;
  height: 64px;
}
.nb .nb__iconfont {
  font-family: navigation-iconfont !important;
  font-size: 36rpx;
  display: block;
  speak: none;
  font-style: normal;
  font-weight: 400;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.nb .nb__i-search:before {
  content: "";
}
.nb .nb__i-home:before {
  content: "";
}
.nb .nb__i-menu:before {
  content: "";
}
.nb .nb__i-back:before {
  content: "";
}
.nb .nb__sticky-wrap {
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 10001;
}
.nb .nb__main {
  height: 64px;
  background: #fff;
}
.nb .nb__icon {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: space-around;
  border: 1px solid hsla(0, 0%, 89.8%, 0.6);
  box-sizing: border-box;
  overflow: hidden;
  background: hsla(0, 0%, 100%, 0.6);
}
.nb .nb__icon .nb-button {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nb .nb__icon .nb-search {
  background: rgba(247, 248, 250, 0.8);
  border: none !important;
}
.nb .nb__icon .nb_home {
  display: flex;
  align-items: center;
}
.nb .nb__icon .nb_home .nb-image {
  width: 100%;
  height: 100%;
}
.nb .nb__icon__split:after {
  content: "";
  width: 1px;
  height: 65%;
  position: absolute;
  left: 50%;
  top: 18%;
  background: hsla(0, 0%, 89.8%, 0.6);
}
.nb .nb__search-text {
  font-size: 12px;
  margin-left: 5px;
  color: #969799;
  overflow: hidden;
}
.nb .nb__title-text {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  box-sizing: border-box;
  font-size: 36rpx;
  font-weight: 500;
  padding-top: 20px;
}
.nb .nb__title-text .title-text {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: center;
  width: 161px;
  font-size: 16px;
}
.nb .nb__placeholder {
  position: absolute;
}
.weapp-top-nav {
  position: fixed;
  z-index: 10000;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.7);
  transition: all 0.5s ease-out;
}
.weapp-top-nav-bg {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.2);
}
.weapp-top-nav-wrapper {
  z-index: 1;
  position: absolute;
  top: 38.4rpx;
  left: 16px;
  padding: 14.4rpx 28.8rpx 28.8rpx;
  background: #fbfbfb;
  max-width: 662.4rpx;
  box-sizing: border-box;
  border-radius: 9.6rpx;
}
.weapp-top-nav-wrapper__after {
  content: "";
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 0 16.8rpx 19.2rpx 16.8rpx;
  border-color: transparent transparent #fff transparent;
  position: absolute;
  left: 88.8rpx;
  top: -19.2rpx;
}
.weapp-top-nav__custom-path,
.weapp-top-nav__sys-path {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.weapp-top-nav__custom-path {
  margin-top: 28.8rpx;
  padding-top: 19.8rpx;
  border-top: 1px solid #ebedf0;
}
.weapp-top-nav__item {
  display: flex;
  align-items: center;
  flex-direction: column;
  min-width: 129.6rpx;
  width: 129.6rpx;
  height: 129.6rpx;
  margin-right: 28.8rpx;
}
.weapp-top-nav__item:nth-child(4n) {
  margin-right: 0;
}
.weapp-top-nav__item-img {
  height: 48rpx;
  width: 48rpx;
}
.weapp-top-nav__item-img_custom {
  height: 60rpx;
  width: 60rpx;
}
.weapp-top-nav__item-image {
  width: 96rpx;
  height: 96rpx;
  min-height: 96rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
}
.weapp-top-nav__item-title {
  height: 33.6rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #646566;
  line-height: 33.6rpx;
  white-space: nowrap;
}
.weapp-top-nav .btn {
  padding: 0;
  border-radius: 0;
  background-color: transparent;
}
.weapp-top-nav .btn:after,
.weapp-top-nav .btn:before {
  border: none;
}
