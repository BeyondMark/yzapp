.t-text {
  display: inline-block;
  text-decoration: inherit;
}
.text-ellipsis-1 {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.location-bar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 16px;
  background: #fff;
}
.location-bar__location {
  height: 24px;
  line-height: 24px;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 14px;
  flex-shrink: 0;
  flex: 1;
  overflow: hidden;
}
.location-bar__text {
  color: #000;
  margin: 0 4px 0 6px;
}
.location-bar__edit {
  display: inline-block;
  font-size: 12px;
  height: 20px;
}
.location-bar__reset {
  color: #323233;
  font-size: 12px;
  flex-shrink: 0;
  padding-left: 20px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.location-bar__reset-icon {
  width: 20px;
  height: 20px;
  margin-right: 2px;
}
.location-bar__reset-aim {
  display: inline-block;
  font-size: 16px;
  color: #323233;
}
.location-bar__reset-text {
  flex-shrink: 0;
  line-height: 24px;
}
