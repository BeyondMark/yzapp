.coupon-list-valid {
  padding-top: 16px;
  color: #333;
  height: 100vh;
}
.coupon-text {
  color: #969799;
  font-size: 12px;
  line-height: 16px;
  padding: 0 20px;
}
.coupon-loading {
  text-align: center;
}
.coupon-list__container {
  padding: 15px 20px;
}
.coupon-list__end {
  text-align: center;
  padding: 25px 0;
  font-size: 12px;
}
.coupon-list__end-tips {
  color: #999;
}
.coupon-list__end-expired {
  color: #d40000;
}
.coupon-list__more {
  display: inline-block;
}
.coupon-list__pull {
  width: 100%;
  height: 35px;
  text-align: center;
}
.coupon-list .empty-content {
  margin-bottom: 100px;
}
.coupon-list .tip {
  font-size: 12px;
  text-align: center;
  padding-bottom: 65px;
  bottom: 90px;
  width: 100%;
}
.coupon-list .tip .goto-invalid-tip {
  color: #969799;
}
.coupon-list .empty-wrapper {
  min-height: calc(100vh - 140px);
}
