.retail-solitaire-share-popup {
  width: 100% !important;
  top: 0 !important;
  transform: translate3d(-50%, 0, 0) !important;
  background-color: transparent !important;
}
.retail-solitaire-share-popup .share-guide-container {
  text-align: center;
}
.retail-solitaire-share-popup .share-guide-container .share-guide-arrow {
  width: 66px;
  height: 117px;
  background-image: url(https://img01.yzcdn.cn/public_files/2019/07/04/b218a1c421117bda16050df68876f3a0.png);
  background-size: 100% 100%;
  margin: 76px 24px 8px auto;
}
.retail-solitaire-share-popup .share-guide-container .share-guide-inner {
  color: #fff;
  text-align: center;
}
.retail-solitaire-share-popup .share-guide-container .share-guide-inner p {
  line-height: 30px;
}
.retail-solitaire-share-popup
  .share-guide-container
  .share-guide-inner
  .guide-tip {
  font-size: 20px;
  font-weight: 500;
  position: relative;
  display: inline-block;
}
.retail-solitaire-share-popup
  .share-guide-container
  .share-guide-inner
  .guide-tip:before {
  content: "";
  display: block;
  position: absolute;
  left: -22px;
  top: -5px;
  width: 16px;
  height: 16px;
  background: url(https://img01.yzcdn.cn/public_files/2019/07/04/64221c3a6a1d3fcc984aab6cb38037e0.png);
}
.retail-solitaire-share-popup
  .share-guide-container
  .share-guide-inner
  .sub-tip {
  font-size: 14px;
}
.solitaire-dialog {
  border-radius: 20px 20px 0 0;
  text-align: center;
