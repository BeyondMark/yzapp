.protocol-check__icon {
  border-radius: 0 !important;
}
.protocol {
  display: flex;
  align-items: flex-start;
}
.protocol-check {
  position: relative;
  flex: 0 0 auto;
  margin-right: 8px;
}
.protocol-check__icon {
  border-radius: 0 !important;
}
.protocol-check__tip {
  position: absolute;
  z-index: 2;
  bottom: 100%;
  left: 0;
  margin-bottom: 6px;
}
.protocol-tip {
  min-width: 7em;
  padding: 4px 6px;
  border-radius: 3px;
  font-size: 12px;
  line-height: 1;
  background: #155bd4;
  color: #fff;
}
.protocol-tip:before {
  content: "";
  position: absolute;
  left: 6px;
  top: 100%;
  width: 0;
  height: 0;
  border: solid transparent;
  border-width: 3px 3px 0 3px;
  border-top-color: #155bd4;
}
.protocol-list {
  flex: 1 1 auto;
  margin-top: -2px;
  word-break: break-all;
  word-wrap: break-word;
  font-size: 12px;
  color: #999;
}
.protocol-txt {
  display: inline;
}
.protocol-txt--link {
  color: #333;
}
