.c9fc5d {
  box-sizing: border-box;
  height: 100%;
  background: #f7f8fa;
  margin: var(--theme-goods-direct-order-popup-card-margin-top, 12px)
    var(--theme-goods-direct-order-popup-card-margin-right, 0)
    var(--theme-goods-direct-order-popup-card-margin-bottom, 12px)
    var(--theme-goods-direct-order-popup-card-margin-left, 0);
}
.fcb2e {
  display: flex;
  position: absolute;
  top: -4px;
  right: calc(
    12px - var(--theme-goods-direct-order-popup-card-margin-right, 0px)
  );
  z-index: 100;
}
.c9fc5d__body {
  min-height: 250px;
  min-height: 40vh;
  max-height: 400px;
  max-height: 70vh;
  overflow-y: auto;
}
.c261ea {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
  box-sizing: border-box;
  width: 100%;
  padding: 0 16px;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  background-color: #fff;
}
.c2ea4d {
  border-radius: var(--theme-radius-button, 20px);
}
.c9fc5d__add,
.c2ea4d {
  height: 40px !important;
  line-height: 38px !important;
  margin: 5px 0;
}
.c9fc5d__disabled-text {
  padding: 20px 0 16px;
  color: #969799;
  font-size: 14px;
  line-height: 20px;
}
.c2fcbb {
  margin-bottom: 12px;
  padding-top: 12px;
  font-size: 14px;
  color: #646566;
}
