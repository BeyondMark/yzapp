.block-top-line {
  position: relative;
  padding-top: 4px;
}
.block-top-line:before {
  position: absolute;
  box-sizing: border-box;
  content: " ";
  pointer-events: none;
  top: -50%;
  right: -50%;
  bottom: -50%;
  left: -50%;
  border: 0 solid #f9f9fa;
  transform: scale(0.5);
  border-top-width: 8px;
  z-index: 1;
}
