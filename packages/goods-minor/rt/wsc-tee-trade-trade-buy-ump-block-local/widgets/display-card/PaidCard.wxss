@keyframes coupon-guide-animate {
  0% {
    transform: scale(0) rotate(4deg);
  }
  40% {
    transform: scale(1.1) rotate(4deg);
  }
  55% {
    transform: scale(0.9) rotate(-3deg);
  }
  70% {
