.long-cell {
  padding: var(--theme-page-cell-padding-top, 10px)
    var(--theme-page-cell-padding-right, 12px)
    var(--theme-page-cell-padding-bottom, 10px)
    var(--theme-page-cell-padding-left, 12px) !important;
}
.c6039e {
  text-align: left;
  height: 75vh;
  border-radius: var(--theme-radius-dialog, 16px)
    var(--theme-radius-dialog, 16px) 0 0 !important;
}
.c6039e .action-sheet__body {
  max-height: none;
}
.c5c2bb {
  font-size: 16px;
  font-weight: 700;
  text-align: center;
  line-height: 44px;
}
.db831 {
  display: flex;
  align-items: center;
  position: absolute;
  padding: 0 16px;
  color: #c5c9cc;
  font-size: 14px;
  height: 100%;
  left: 0;
}
.c9a13f {
  height: 100%;
  overflow: hidden;
}
.a497a {
  height: calc(100% - 40px);
  display: flex;
  transition: transform 0.4s cubic-bezier(0.84, 0.84, 0.32, 0.32);
}
.a497a .t-hairline--bottom:after {
  border: none;
}
.ac0cd {
  flex: none;
  width: 100%;
}
.c6039e__hidden-item {
  display: none;
}
.e3901 .t-icon-checked {
  position: absolute !important;
  color: var(--main-bg, #323233);
}
.c6039e .t-picker-column__item--selected {
  font-weight: 500;
}
.c6039e .t-tree-select__item {
  font-weight: var(--theme-common-field-font-weight, 500);
}
.c6039e .t-tree-select__selected {
  font-size: 18px;
}
.c6039e .t-sidebar-item {
  border-left-width: 4px;
}
.c12dbc,
.c654c2 {
  font-weight: var(--theme-common-field-font-weight, bold);
}
.c12dbc {
  color: var(--main-bg, #323233);
}
.c12dbc:after {
  background-color: var(--main-bg, #323233) !important;
}
.a7b07 {
  color: var(--main-bg, #323233);
  font-weight: var(--theme-common-field-font-weight, bold);
}
.dfaa2 {
  color: var(--general, #323233);
}
