.cart-bottom {
  background: #fff;
  position: fixed;
  bottom: 0;
  z-index: 10;
  width: 100%;
  box-sizing: border-box;
}
.cart-bottom:after {
  position: absolute;
  box-sizing: border-box;
  transform-origin: center;
  content: " ";
  pointer-events: none;
  top: 0;
  right: -50%;
  left: -50%;
  border: 0 solid #ebedf0;
  border-top-width: 1px;
  transform: scale(0.5);
}
.tips {
  font-weight: 400;
  color: #666;
  display: inline-block;
}
.c46ead {
  box-sizing: border-box;
  font-size: 12px;
  line-height: 16px;
  color: #999;
