    </view>
    <view style="{{_pageStyle}}">
        <view>
            <view class="c964a4">
                <view class="c964a4">
                    <a bind:_init="_tic" nr="true" rmId="b4-4" wx:if="{{RM&&RM['b4-4']}}"></a>
                </view>
                <view class="c964a4">
                    <b bind:_init="_tic" nr="true" rmId="b4-15" wx:if="{{RM&&RM['b4-15']}}"></b>
                    <c bind:_init="_tic" nr="true" rmId="b4-11" wx:if="{{RM&&RM['b4-11']}}"></c>
                </view>
                <view class="c964a4">
                    <d bind:_init="_tic" nr="true" rmId="b4-7" wx:if="{{RM&&RM['b4-7']}}"></d>
                </view>
                <view class="c964a4">
                    <e bind:_init="_tic" nr="true" rmId="b4-8" wx:if="{{RM&&RM['b4-8']}}"></e>
                </view>
                <view class="c964a4">
                    <f bind:_init="_tic" nr="true" rmId="b4-9" wx:if="{{RM&&RM['b4-9']}}"></f>
                </view>
                <view class="c964a4">
                    <g bind:_init="_tic" nr="true" rmId="b4-5" wx:if="{{_post&&RM&&RM['b4-5']}}"></g>
                </view>
                <view class="c964a4">
                    <h bind:_init="_tic" nr="true" rmId="b4-1" wx:if="{{_post&&RM&&RM['b4-1']}}"></h>
                </view>
            </view>
            <view class="c964a4">
