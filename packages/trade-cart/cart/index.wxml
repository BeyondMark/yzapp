                    <n bind:_init="_tic" nr="true" rmId="b4-22" wx:if="{{_normal&&RM&&RM['b4-22']}}"></n>
                </view>
            </view>
            <o bind:_init="_tic" data-ref="van-toast" isPageDefault="{{true}}"></o>
            <p bind:_init="_tic" data-ref="van-dialog" isPageDefault="{{true}}"></p>
        </view>
    </view>
    <r bind:_init="_tic"></r>
