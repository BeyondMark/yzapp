.count-box {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.count-box,
.viewport {
  overflow: hidden;
}
.column-wrap {
  width: 100%;
  transform: translateZ(0);
  transition: all 1s cubic-bezier(0, 1, 0, 1);
}
.item {
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 40px;
  font-weight: 700;
}
