.list-container {
  margin: 15px;
  overflow: hidden;
  padding: 0 15px 0 15px;
  border-radius: 16px;
  box-sizing: border-box;
  background-color: #fff;
  position: relative;
}
.list-container .list-title {
  border-radius: 0 0 16px 16px;
  font-size: 14px;
  line-height: 20px;
  text-align: center;
  padding: 6px 0;
  width: 220px;
  margin: auto;
  color: #f34900;
  background-color: rgba(243, 73, 0, 0.0980392157);
}
