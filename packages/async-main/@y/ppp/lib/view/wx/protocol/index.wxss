.protocol-title {
  display: none;
  font-size: 15px;
  font-weight: 700;
  color: #191919;
  align-items: center;
}
.protocol-content {
  color: #191919;
}
.protocol-content__skt {
  position: relative;
  left: -16px;
  margin: 12px -32px 0 0;
}
.protocol-content__header {
  padding: 0 0 8px;
  font-size: 20px;
  line-height: 26px;
  font-weight: 700;
  overflow: hidden;
}
.protocol-content__body {
  font-size: 14px;
  line-height: 20px;
  color: #707070;
}
.protocol-content__protocols {
  margin-top: 4px;
  line-height: 20px;
  font-size: 14px;
  color: #576b95;
  display: block;
}
