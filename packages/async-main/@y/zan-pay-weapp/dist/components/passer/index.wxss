.b {
  height: 80vh;
}
.header {
  height: 44px;
  padding: 10px 16px;
  box-sizing: border-box;
  text-align: left;
  position: relative;
}
.center {
  text-align: center;
}
.content {
  font-size: 18px;
  line-height: 24px;
}
.root {
  box-sizing: border-box;
  position: relative;
  padding: 38px 16px 282px;
}
.link {
  font-size: 12px;
  line-height: 16px;
  position: absolute;
  top: 98px;
  right: 20px;
}
.link,
.link:active,
.link:hover,
.link:link,
.link:visited {
  color: #3b69d2;
  text-decoration: none;
}
