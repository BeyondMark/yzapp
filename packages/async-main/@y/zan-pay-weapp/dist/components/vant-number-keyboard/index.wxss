.van-number-keyboard {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  animation-timing-function: ease-out;
  user-select: none;
}
.van-number-keyboard__body {
  box-sizing: border-box;
}
.van-number-keyboard--custom .van-number-keyboard__body {
  padding-right: 25%;
}
