.addbd {
  display: flex;
  align-items: stretch;
  height: 40px;
  list-style-type: none;
  background-color: #fff;
  border-left: 1px solid #f1f1f1;
  border-right: 1px solid #f1f1f1;
  font-size: 14px;
  color: #717171;
}
.c0a228 {
  flex: 1;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  user-select: none;
}
.c0f74f {
  color: var(--icon, #323233);
}
.c73a6c {
  position: relative;
}
.df20d {
  top: -4px;
  border-top: 1px solid #bbb;
  border-left: 1px solid #bbb;
}
.df20d,
.c02d8f {
  content: "";
  display: block;
  position: absolute;
  left: 5px;
  width: 3px;
  height: 3px;
  transform: rotate(45deg);
}
.c02d8f {
  bottom: -5px;
  border-bottom: 1px solid #bbb;
  border-right: 1px solid #bbb;
}
.c0f74f .c73a6c--ascend .df20d,
.c0f74f .c73a6c--descend .c02d8f {
  border-color: var(--icon, #323233);
}
.b64a1 {
  width: 48px;
  height: 40px;
  display: flex;
  justify-content: center;
  align-items: center;
}
