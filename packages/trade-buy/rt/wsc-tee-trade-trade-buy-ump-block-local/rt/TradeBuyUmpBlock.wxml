<view class="trade-buy-ump-block" wx:if="{{dataLoaded}}">
    <block wx:if="{{showPlusPop}}">
        <f bind:_init="_tic" bind:closePlusSwipe="_p" data-t="{{ closePlusSwipe:{en:'closePlusSwipe'} }}" payPrice="{{payPrice}}" plusBuyGoodsList="{{plusBuyGoodsList}}" rmId="{{rmId}}" selectedCount="{{selectedCount}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></f>
    </block>
    <block wx:if="{{showPlusBuy}}">
        <e bind:_init="_tic" payPrice="{{payPrice}}" plusBuyGoodsList="{{plusBuyGoodsList}}" rmId="{{rmId}}" selectedCount="{{selectedCount}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></e>
    </block>
    <m bind:_init="_tic">
        <a bind:_init="_tic" info="{{umpBlockBase}}" postage="{{selected}}" rmId="{{rmId}}" showDisplayCardPrice="{{showDisplayCardPrice}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></a>
        <c bind:_init="_tic" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></c>
        <block wx:if="{{isShowDeliveryCouponCell&&isShow}}">
            <g bind:_init="_tic" opt="{{deliveryCouponOpt}}" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></g>
        </block>
        <h bind:_init="_tic" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></h>
        <block wx:if="{{isShowCouponCell}}">
            <g bind:_init="_tic" opt="{{couponOpt}}" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></g>
        </block>
        <k bind:_init="_tic" color="{{themeColors.general}}" kdtId="{{kdtId}}" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></k>
        <block wx:if="{{orderPromotions.length}}">
            <d bind:_init="_tic" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></d>
        </block>
        <block wx:if="{{points.usePoints}}">
            <i bind:_init="_tic" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></i>
        </block>
        <block wx:if="{{!!pointDesc&&!orderCreated}}">
            <j bind:_init="_tic" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></j>
        </block>
        <b bind:_init="_tic" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></b>
        <o bind:_init="_tic" bind:confirmBestPrice="_p" bind:showRewardPopup="_p" data-t="{{ showRewardPopup:{en:'showRewardPopup'},confirmBestPrice:{en:'confirmBestPrice'} }}" promotionInfo="{{promotionInfo}}" wx:if="{{hasReward}}"></o>
        <n bind:_init="_tic" price="{{orderFinalPrice}}" wx:if="{{showAmount}}"></n>
    </m>
    <l bind:_init="_tic" rmId="{{rmId}}" useCustomTitleSlot="{{true}}" wx:if="{{rmId==='b36-27'}}"></l>
</view>
