.d5a4c {
  padding: 10px 12px 0 !important;
}
.c4269e {
  justify-content: flex-end;
}
.c447b7,
.c4269e {
  display: flex;
  align-items: center;
}
.c447b7 {
  word-wrap: break-word;
  color: #323233;
  line-height: 20px;
}
.c447b7:last-child {
  margin-left: 25px;
}
.baec6 {
  flex-shrink: 0;
  position: relative;
  width: 16px;
  height: 16px;
  border: 1px solid #7d7e80;
  border-radius: 50%;
  box-sizing: border-box;
  margin-bottom: 0;
  margin-right: 10px;
}
.baec6.c76a5 {
  border-color: var(--general);
}
.baec6.c76a5:after {
  content: "";
  position: absolute;
  top: 3px;
  left: 3px;
  width: 8px;
  height: 8px;
  border-radius: 50%;
  background-color: var(--general);
}
.cb5e1 {
  padding: 0 12px;
  font-size: 12px;
  font-weight: 400;
  line-height: 20px;
  text-align: right;
  color: red;
}
