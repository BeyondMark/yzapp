.dc-course-group {
  display: -webkit-flex;
  display: flex;
  width: 100%;
}
.dc-course-group .edu-goods-tags-top--fixed {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 110;
  width: 100%;
}
.dc-course-group.wrap {
  flex-flow: column wrap;
}
.dc-course-group.nowrap {
  flex-flow: row nowrap;
}
.dc-course-group .dc-course-group-goods.template_type0 {
  width: 100%;
}
.dc-course-group .dc-course-group-goods.template_type1 {
  width: calc(100% - 88px);
}
