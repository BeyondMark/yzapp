.dc-reward-points {
  position: relative;
  overflow: hidden;
}
.dc-reward-points .card-container {
  overflow: hidden;
}
.reward-card {
  color: #323233;
}
.reward-card .title {
  font-size: 14px;
  font-weight: 500;
  line-height: 24px;
}
.reward-card .detail,
.reward-card .detail .points {
  display: -webkit-flex;
  display: flex;
}
.reward-card .detail .points {
  align-items: flex-end;
  line-height: 26px;
}
.reward-card .detail .points .num {
  font-size: 24px;
  font-weight: 500;
}
.reward-card .detail .points .all {
  margin-left: 2px;
  font-size: 12px;
  font-weight: 400;
  line-height: 18px;
}
.mode-one.reward-card {
  position: relative;
  padding: 12px 16px;
  overflow: hidden;
}
.mode-one.reward-card .detail {
  align-items: center;
  margin-top: 24px;
  height: 24px;
}
.mode-one.reward-card .detail .points {
  margin-left: 4px;
}
.mode-one.reward-card .detail .img {
  width: 30px;
  height: 30px;
}
.mode-one.reward-card .description {
  margin: 5px 0;
  font-size: 12px;
  line-height: 18px;
  color: #969799;
}
.mode-one.reward-card .process {
  width: 44%;
  background-color: rgba(242, 243, 245, 0.6);
}
.mode-one.reward-card .card-background {
  position: absolute;
  right: 0;
  bottom: -15px;
}
.mode-one.reward-card .card-background .img {
  width: 150px;
  height: 150px;
  opacity: 0.3;
}
.mode-two.reward-card {
  padding: 12px 16px 4px 16px;
}
.mode-two.reward-card .detail {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  margin: 12px 0;
  font-size: 20px;
  font-weight: 500;
  line-height: 26px;
}
.mode-two.reward-card .detail .name {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-right: 20px;
}
.mode-two.reward-card .reward-item-container {
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
}
.mode-two.reward-card .reward-item-container .reward-item {
  margin: 0 calc(5vw - var(--page-margin) * 2 / 10 / 2 - 16.6px);
  margin-bottom: 8px;
}
.mode-two.reward-card .reward-item-container .img {
  width: 30px;
  height: 30px;
}
