.tee-image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.item-placeholder {
  font-size: 20px;
  font-family: Arial;
  line-height: 30px;
}
.unauthorized__wrapper {
  background-color: inherit;
  border-radius: 8px;
  display: -webkit-flex;
  display: flex;
}
.unauthorized__wrapper.horizontal {
  justify-content: space-between;
}
.unauthorized__wrapper.vertical {
  flex-direction: column;
  text-align: center;
  align-items: center;
  padding: 0 16px;
  padding-top: 48px;
}
.unauthorized__wrapper.hello {
  justify-content: space-between;
  align-items: center;
}
.member-text.horizontal {
  margin-right: 24px;
}
.member-text.vertical {
  margin-top: 16px;
}
.title {
  font-weight: 500;
  font-size: 16px;
  line-height: 24px;
  color: var(--realTextColor);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.title.horizontal {
  max-width: 405rpx;
}
.title.vertical {
  max-width: 259.95rpx;
}
.title.hello {
  font-size: 14px;
  font-weight: 400;
  line-height: 20px;
}
.subtitle {
  font-weight: 400;
  font-size: 10px;
  line-height: 14px;
  color: var(--realTextColor);
  opacity: 0.5;
}
.subtitle.vertical {
  margin-top: 8px;
}
.subtitle.hello {
  color: var(--realTextColor);
  opacity: 1;
  font-size: 16px;
  font-weight: 500;
  line-height: 24px;
  margin-top: 4px;
}
.action {
  display: -webkit-flex;
  display: flex;
  width: 88px;
  height: 32px;
  font-weight: 400;
  font-size: 12px;
  justify-content: center;
  align-items: center;
  border-radius: 16px;
  color: #fff;
  background-color: #a8865d;
}
.action__wrapper {
  margin-top: 30px;
  margin-bottom: 70px;
}
.avatar {
  flex-shrink: 0;
  width: 48px;
  height: 48px;
  border-radius: 50%;
  border: 2px solid #fff;
  overflow: hidden;
  background: #fff;
}
