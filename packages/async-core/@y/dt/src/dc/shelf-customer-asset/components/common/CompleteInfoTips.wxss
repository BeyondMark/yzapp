.item-placeholder {
  font-size: 20px;
  font-family: Arial;
  line-height: 30px;
}
.container {
  background: #fff;
  border-radius: 8px;
  margin-top: 12px;
  padding: 10px 12px 10px 16px;
  padding-top: 10px !important;
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
}
.benefits-text {
  font-style: normal;
  font-weight: 500;
  max-width: 80%;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.actions,
.benefits-text {
  font-size: 14px;
  line-height: 24px;
}
.actions {
  font-weight: 400;
  color: #666;
}
