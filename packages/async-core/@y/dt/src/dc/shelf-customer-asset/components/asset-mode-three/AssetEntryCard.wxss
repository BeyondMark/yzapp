.tee-image {
  display: inline-block;
  width: 100%;
  height: 100%;
}
.entries {
  display: -webkit-flex;
  display: flex;
  height: 100%;
  flex-direction: column;
}
.entry {
  position: relative;
  flex: 1;
  width: 100%;
  height: auto;
  padding-bottom: 92%;
  background-color: #fff;
  overflow: hidden;
}
.entry .entry-image {
  position: absolute;
}
