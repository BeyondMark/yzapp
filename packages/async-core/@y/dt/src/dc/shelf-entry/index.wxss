.table-code-layout {
  margin-top: 10px;
}
.table-code-layout .table-code {
  height: 44px;
  position: relative;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  background-color: #fff;
  margin: 10px 16px 0;
  padding-left: 15px;
  box-shadow: 0 2px 10px hsla(0, 0%, 49%, 0.16);
  border-radius: 8px;
}
.table-code-layout .table-code .food {
  height: 24px;
  margin-right: 10px;
}
.table-code-layout .type-1 {
  box-shadow: none;
}
.table-code-layout .type-3 {
  box-shadow: none;
  border: 1px solid #ebedf0;
}
.table-code-layout .type-4 {
  box-shadow: none;
  background-color: transparent;
}
.table-code-layout .border-type-1 {
  border-radius: 0;
}
.entry-layout {
  display: -webkit-flex;
  display: flex;
  padding: 6px var(--pageMargin, 16px);
  background-color: var(--cardBgColor, transparent);
}
.entry-layout.vertical {
  flex-direction: column;
}
.entry-layout.horizontal {
  flex-direction: row;
  justify-content: space-between;
}
.entry-layout.horizontal .entry-card:not(:last-child) {
  margin-right: 10px;
}
.entry-layout.type-three {
  flex-direction: row;
}
.entry-layout .type-three--right {
  flex: 1;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
}
.entry-layout.type-five .type-five--bottom-row,
.entry-layout.type-five .type-five--top-row {
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
}
.entry-layout .entry-card-custom {
  display: -webkit-flex;
  display: flex;
  flex: 1;
}
