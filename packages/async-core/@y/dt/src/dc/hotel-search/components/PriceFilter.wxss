.t-text {
  display: inline-block;
  text-decoration: inherit;
}
.filter-content-title {
  width: 100%;
  padding: 20px 0;
  text-align: center;
}
.filter-content-range,
.filter-content-range-slider {
  padding: 16px;
}
.filter-content-range-detail {
  display: -webkit-flex;
  display: flex;
  justify-content: space-around;
  color: #969799;
  padding: 20px 16px 8px 16px;
}
.filter-content-range-detail-text {
  text-align: left;
  flex: 1;
}
.filter-content .van-sku-actions {
  display: -webkit-flex;
  display: flex;
  padding: 8px 16px;
}
.filter-content .van-sku-actions-reset {
  flex: 1;
  border-top-left-radius: 50px;
  border-bottom-left-radius: 50px;
  overflow: hidden;
}
.filter-content .van-sku-actions-complete {
  flex: 1;
  border-top-right-radius: 50px;
  border-bottom-right-radius: 50px;
  overflow: hidden;
}
