.t-text {
  text-decoration: inherit;
}
.t-text,
.tee-image {
  display: inline-block;
}
.tee-image {
  width: 100%;
  height: 100%;
}
.popup-title {
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  flex-shrink: 0;
  text-align: center;
  padding: 0 16px;
  height: 44px;
  line-height: 44px;
  background-color: #fff;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 10;
}
.shop-name {
  flex: 1;
  font-size: 16px;
  font-weight: 500;
  transform: translateX(12tpx);
}
.popup-content {
  overflow-y: auto;
  height: calc(80vh - 100px);
  box-sizing: border-box;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.ump-block {
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  margin-top: 32px;
  padding: 0 16px;
}
.title {
  font-size: 14px;
  color: #313133;
  font-weight: 700;
  margin-bottom: 8px;
}
.body {
  font-size: 12px;
  color: #636566;
}
.body.info {
  line-height: 20px;
  margin-bottom: 5px;
}
.body.shop-certification {
  color: #323233;
  font-weight: 500;
  line-height: 20px;
}
.body.shop-certification > view:first-child {
  margin-right: 4px;
}
.more {
  text-align: center;
  color: #969799;
  font-size: 12px;
  margin: 8px 0 16px;
}
.more-text {
  margin-right: 4px;
}
.activity {
  display: block;
}
.activity:not(:last-child) {
  margin-bottom: 12px;
}
.logo {
  width: 20px;
  height: 20px;
  vertical-align: bottom;
  margin-left: 3px;
}
