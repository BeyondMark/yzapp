@font-face {
  font-family: venue;
  src: url(https://file.yzcdn.cn/upload_files/yz-file/2021/05/12/FpqYbzRlH2r5XLnxbtyl_iWN5rRI.ttf);
}
.dc-venue-banner {
  position: relative;
}
.dc-venue-banner-title_container {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  text-align: center;
}
.dc-venue-banner-title {
  top: 34px;
  font-family: venue;
  color: #fd0931;
  font-size: 54px;
  width: 247px;
  letter-spacing: -3px;
}
.dc-venue-banner-subtitle,
.dc-venue-banner-title {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  margin: 0;
  text-align: center;
  word-break: break-word;
}
.dc-venue-banner-subtitle {
  top: 100px;
  display: inline-block;
  font-size: 18px;
  color: #fff;
  font-weight: 700;
  height: 24px;
  line-height: 24px;
  background-color: #f64231;
  max-width: 220px;
  min-width: 200px;
  border-radius: 999px;
}
